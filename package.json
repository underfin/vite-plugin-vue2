{
  "name": "vite-plugin-vue2",
  "version": "1.0.0",
  "description": "Vite plugin for Vue2",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsc -w -p . --incremental --skipLibCheck",
    "test": "jest --clearCache && jest --runInBand --forceExit",
    "build": "rm -rf dist && tsc -p . --skipLibCheck",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "prepublishOnly": "yarn build && yarn changelog",
    "postpublish": "git add CHANGELOG.md && git commit -m 'chore: changelog [ci skip]'"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "vite",
    "vue2"
  ],
  "author": "underfin",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/underfin/vite-plugin-vue2"
  },
  "bugs": {
    "url": "https://github.com/underfin/vite-plugin-vue2/issues"
  },
  "homepage": "https://github.com/underfin/vite-plugin-vue2/tree/master/#readme",
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write"
    ],
    "*.ts": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@babel/core": "^7.11.6",
    "@rollup/pluginutils": "^4.1.0",
    "@vue/compiler-sfc": "^3.0.0-rc.5",
    "@vue/component-compiler-utils": "^3.2.0",
    "babel-helper-vue-jsx-merge-props": "^2.0.3",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-transform-vue-jsx": "^3.7.0",
    "babel-preset-env": "^1.7.0",
    "chalk": "^4.1.0",
    "debug": "^4.3.1",
    "fs-extra": "^9.0.1",
    "hash-sum": "^2.0.0",
    "prettier": "^2.0.5",
    "querystring": "^0.2.0",
    "rollup": "^2.35.1",
    "vite": "2.0.0-beta.4",
    "vue": "^2.6.11",
    "vue-template-compiler": "^2.6.11"
  },
  "peerDependencies": {
    "vite": "2.0.0-beta.4"
  },
  "devDependencies": {
    "@types/debug": "^4.1.5",
    "@types/fs-extra": "^9.0.1",
    "@types/hash-sum": "^1.0.0",
    "@types/jest": "^26.0.4",
    "@types/node": "^14.0.14",
    "@types/puppeteer": "^3.0.1",
    "conventional-changelog-cli": "^2.0.34",
    "execa": "^4.0.2",
    "jest": "^26.1.0",
    "lint-staged": "^10.2.11",
    "puppeteer": "^3.0.0",
    "ts-jest": "^26.1.1",
    "typescript": "^4.1.2",
    "yorkie": "^2.0.0"
  }
}
